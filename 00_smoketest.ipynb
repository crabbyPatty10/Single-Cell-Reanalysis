    "import scanpy as sc\n",
    "import anndata as ad\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "\n",
    "X = np.random.poisson(1.0, size=(100, 50))\n",
    "adata = ad.AnnData(X)\n",
    "adata.obs[\"sample_id\"] = [\"s1\"]*50 + [\"s2\"]*50\n",
    "adata.var_names = [f\"gene{i}\" for i in range(50)]\n",
    "print(adata)"
